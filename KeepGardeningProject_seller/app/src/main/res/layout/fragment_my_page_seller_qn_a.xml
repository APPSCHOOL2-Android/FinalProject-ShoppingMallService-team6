<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".UI.MyPageSellerQnA.MyPageSellerQnAFragment">

    <com.google.android.material.appbar.MaterialToolbar
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        style="@style/Theme.KeepGardeningProject_seller.Toolbar"
        android:id = "@+id/materialToolbar_seller_question"/>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="@dimen/padding_size_middle">

        <androidx.recyclerview.widget.RecyclerView
            android:id = "@+id/recyclerView_seller_question"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

    </ScrollView>




</LinearLayout>